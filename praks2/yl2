#!/bin/bash
#
#  kasutajate nimekiri kujul Perenimi, Eesnimi
#
# skripti algus
who | cut -f1 -d " " | cut -f2 -d"." > perenimi # l천ikame perenimi -see on 2. element peale punkti ja salvestame faili
who | cut -f1 -d " " | cut -f1 -d"." > eesnimi # l천ikame eesnime -see on 1.element ennem punkti ja salvestame faili
paste -d"." perenimi eesnimi > nimed # kleebime need failid vastavas j채rjekorras kokku
cat nimed | awk -F"." '{print $1"."$2}'
#
# skripti l천pp

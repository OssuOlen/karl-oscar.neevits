#!/bin/bash
#skripti algus
echo -n "Mitu reisijat on grupis?"
read reisijad #kysib sisendit
echo -n "Mitu kohta on igas bussis?: "
read kohad #kysib sisendit
Arvutus1=`expr $reisijad / $kohad` #arvutus
Arvutus2=`expr $reisijad % $kohad` #arvutus
echo "bussides on $Arvutus1 t채idetud kohta ja maha j채채b $Arvutus2 inimest"
#
